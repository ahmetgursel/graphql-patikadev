query getAllUsers {
  users {
    id
    fullName
    profile_photo
    posts {
      id
      title
    }
    comments {
      id
      text
    }
  }
}

query getUser {
  user(id:"63200a6463fdcc7e702a9ca1") {
    fullName
    profile_photo
    posts {
      id
      title
    }
    comments {
      id
      text
    }
  }
}

mutation createUser {
  createUser(data: {
    age: 31
    fullName: "Mukaddes GÃ¼rsel"
    profile_photo: "https://joeschmoe.io/api/v1/random"
  }) {
    id
    fullName
    age
    profile_photo    
  }
}

mutation updateUser {
  updateUser(
    id: "632031df87c4f4c7bc9fdd2c"
    data: {
      age: 56
    }
  ) {
    id
    fullName
    age
    profile_photo
  }
}

mutation deleteUser {
  deleteUser(id: "632031df87c4f4c7bc9fdd2c") {
    id
    fullName
    age
    profile_photo
  }
}

mutation deleteAllUsers{
  deleteAllUsers {
    count
  }
}

query getAllPosts {
  posts{
    id
    title
    description
    short_description
    cover
    user {
      id
      fullName
    }
    comments {
      id
      text
    }
  }
}

query getPost {
  post(id: "6320129c63fdcc7e702a9cac") {
    id
    title
    description
    short_description
    cover
    user {
      id
      fullName
    }
    comments {
      id
      text
      user {
        fullName
      }
    }
  }
}

query getAllComments {
  comments {
    id
    text
    user {
      id
      fullName
    }
    post {
      id
      title
    }
  }
}

query getComment {
  comment(id: "6320246863fdcc7e702a9ce6") {
    text
    user {
      id
      fullName
    }
  }
}