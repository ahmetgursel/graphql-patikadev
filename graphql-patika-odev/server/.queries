query getAllUsers{
  users{
    id
    username  
    email
    events {
      id
    }
  }
}

query getOneUser {
  user(id: "1") {
    id
    username
    email
    events {
      id
    }
  }
}

mutation createUser {
  createUser(data: {
    username: "Example User"
    email: "user@example.com"
  }) {
    id
    username
    email
  }
}

mutation updateUser {
  updateUser(id: "1", data: {
    username: "Updated User"
    email: "updated@user.com"
  }) {
    id
    username
    email
  }
}

mutation deleteOneUser {
  deleteUser(id: "2") {
    id
    username
    email
  }
}

mutation deleteAllUser {
  deleteAllUsers {
    count
  }
}

subscription createdUser {
  userCreated {
    id
    username
    email
  }
}

subscription updatedUser {
  userUpdated {
    id
    username
    email
  }
}

subscription deletedUser {
  userDeleted {
    id
    username
    email
  }
}

